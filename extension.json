{
	"name": "SemanticCite",
	"version": "3.0.0",
	"author": [
        	"James Hong Kong"
	],
	"url": "https://github.com/SemanticMediaWiki/SemanticCite/",
	"descriptionmsg": "sci-desc",
	"namemsg": "sci-name",
	"license-name": "GPL-2.0-or-later",
	"type": "semantic",
	"requires": {
		"MediaWiki": ">= 1.39",
		"extensions": {
			"SemanticMediaWiki": ">= 4.0"
		}
	},
	"AutoloadNamespaces": {
		"SCI\\": "src"
	},
	"AutoloadClasses": {
		"SemanticCite": "SemanticCite.php"
	},
	"MessagesDirs": {
		"SemanticCite": [
			"i18n"
		]
	},
	"callback": "SemanticCite::initExtension",
	"ExtensionFunctions": [
		"SemanticCite::onExtensionFunction"
	],
	"load_composer_autoloader": true,
	"manifest_version": 2
}
